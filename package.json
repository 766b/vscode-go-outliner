{
    "name": "go-outliner",
    "displayName": "Go Outliner",
    "description": "Go code outline explorer",
    "version": "0.0.5",
    "publisher": "766b",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:go",
        "workspaceContains:*.go"
    ],
    "main": "./out/extension",
    "repository": {
        "type": "git",
        "url": "https://github.com/766b/vscode-go-outliner.git"
    },
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "typeView",
                    "name": "Types",
                    "when": "showGoOutlinerTypes"
                },
                {
                    "id": "funcView",
                    "name": "Functions",
                    "when": "showGoOutlinerFuncs"
                },
                {
                    "id": "varView",
                    "name": "Variables",
                    "when": "showGoOutlinerVars"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}